{
  "name": "rs-app-react",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://sekaa4.github.io/RS-App-React/",
  "type": "module",
  "scripts": {
    "make:redirects": "echo /* /index.html 200 > dist/_redirects",
    "dev": "vite",
    "build": "tsc --noEmit && vite build && npm run",
    "preview": "vite preview",
    "test": "vitest --reporter=verbose",
    "coverage": "vitest run --coverage --reporter=verbose",
    "lint": "eslint --debug src/",
    "lint:write": "eslint src/ --fix",
    "prettier": "npx prettier --write ./",
    "deploy": "npm run build && npx gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/coverage-istanbul": "^0.29.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "^4.2.3",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,ts,tsx,jsx}": [
      "npm run lint:write",
      "git add"
    ]
  }
}
